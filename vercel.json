{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/en/:path*",
      "destination": "https://thecontinuum-en.vercel.app/:path*"
    },
    {
      "source": "/zh/:path*",
      "destination": "https://the-continuum-zh.vercel.app/:path*"
    },
    {
      "source": "/",
      "destination": "/en/"
    }
  ]
}

